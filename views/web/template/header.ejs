        <script src="/assets/theme/js/device.js?jsv=0.3"></script>
        <script src="/assets/theme/js/flashdetect.min.js"></script>
        <script src="/assets/theme/js/util.js?v=1.18"></script>
        <script src="/assets/theme/js/common.js?v=1.21"></script>
