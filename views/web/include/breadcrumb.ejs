<% 
var title = (title) ? title : "";
var homeURL = (homeURL) ? homeURL : "/";

%> 
<div
  class="gen-breadcrumb"
  style="background-image: url('images/background/asset-25.jpg')"
>
  <div class="container">
    <div class="row align-items-center">
      <div class="col-lg-12">
        <nav aria-label="breadcrumb">
          <div class="gen-breadcrumb-title">
            <h1><%=title%></h1>
          </div>
          <div class="gen-breadcrumb-container">
            <ol class="breadcrumb">
              <li class="breadcrumb-item">
                <a href="<%=homeURL%>"><em class="fas fa-home mr-2"></em>Home</a>
              </li>
              <li class="breadcrumb-item active"><%=title%></li>
            </ol>
          </div>
        </nav>
      </div>
    </div>
  </div>
</div>
